{
  "name": "todo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build-tailwind": "npx @tailwindcss/cli -i ./tailwind.input.css -o ./src/main/resources/static/libs/tailwind/tailwind.min.css --minify",
    "copy-shoelace": "mkdir -p src/main/resources/static/libs/shoelace-style && cp -R node_modules/@shoelace-style/shoelace/cdn/* src/main/resources/static/libs/shoelace-style/",
    "copy-axios": "mkdir -p src/main/resources/static/libs/axios && cp node_modules/axios/dist/axios.min.js src/main/resources/static/libs/axios/",
    "copy-chartjs": "mkdir -p src/main/resources/static/libs/chartjs && cp node_modules/chart.js/dist/chart.umd.js src/main/resources/static/libs/chartjs/",
    "copy-alpinejs": "mkdir -p src/main/resources/static/libs/alpinejs && cp node_modules/alpinejs/dist/cdn.min.js src/main/resources/static/libs/alpinejs/alpine.min.js",
    "copy-exceljs": "mkdir -p src/main/resources/static/libs/exceljs && cp node_modules/exceljs/dist/exceljs.min.js src/main/resources/static/libs/exceljs/",
    "copy-htmx": "mkdir -p src/main/resources/static/libs/htmx && cp node_modules/htmx.org/dist/htmx.min.js src/main/resources/static/libs/htmx/",
    "copy-listjs": "mkdir -p src/main/resources/static/libs/listjs && cp node_modules/list.js/dist/list.min.js src/main/resources/static/libs/listjs/",
    "copy-vue": "mkdir -p src/main/resources/static/libs/vue && cp node_modules/vue/dist/vue.global.prod.js src/main/resources/static/libs/vue/",
    "copy-vuetify": "mkdir -p src/main/resources/static/libs/vuetify && cp node_modules/vuetify/dist/vuetify.min.css src/main/resources/static/libs/vuetify/ && cp node_modules/vuetify/dist/vuetify.min.js src/main/resources/static/libs/vuetify/",
    "copy-mdi": "mkdir -p src/main/resources/static/libs/mdi && cp node_modules/@mdi/font/css/materialdesignicons.min.css src/main/resources/static/libs/mdi/ && mkdir -p src/main/resources/static/libs/fonts && cp node_modules/@mdi/font/fonts/* src/main/resources/static/libs/fonts/",
    "copy-toastui": "mkdir -p src/main/resources/static/libs/toastui && cp node_modules/@toast-ui/editor/dist/toastui-editor.min.* src/main/resources/static/libs/toastui/",
    "copy-jsoneditor": "mkdir -p src/main/resources/static/libs/jsoneditor && cp node_modules/jsoneditor/dist/* src/main/resources/static/libs/jsoneditor/",
    "copy-libs": "npm run build-tailwind && npm run copy-shoelace && npm run copy-alpinejs && npm run copy-exceljs && npm run copy-htmx && npm run copy-chartjs && npm run copy-listjs && npm run copy-vue && npm run copy-vuetify && npm run copy-mdi && npm run copy-axios"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@shoelace-style/shoelace": "^2.20.1",
    "axios": "^1.8.4",
    "alpinejs": "^3.14.9",
    "chart.js": "^4.4.8",
    "exceljs": "^4.4.0",
    "htmx.org": "2.0.4",
    "list.js": "^2.3.1",
    "vue": "^3.5.13",
    "vuetify": "^3.8.1",
    "@mdi/font": "^7.4.47",
    "@toast-ui/editor": "^3.2.2",
    "jsoneditor": "^10.2.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.3",
    "tailwindcss": "^4.1.3"
  }
}
